{
   "name": "gieenm-tools-nodejs",
   "version": "0.1.0",
   "description": "The Network Manager Tools for GIEE.",
   "main": "index.js",
   "scripts": {
      "test": "echo \"Error: no test specified\" && exit 1",
      "start": "ts-node index.ts",
      "dev": "ts-node-dev --respawn --transpile-only --ignore-watch \"node_modules .git\" -- index.ts",
      "dev:detail": "ts-node-dev --respawn --transpile-only --debug --ignore-watch \"node_modules .git\" -- index.ts",
      "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/.bin/typeorm",
      "seeder:create": "npm run typeorm -- migration:create --config ormconfig.ts -c seeder -n ",
      "seeder:generate": "npm run typeorm -- migration:generate --config ormconfig.ts -c seeder -n ",
      "seeder:run": "npm run typeorm -- migration:run --config ormconfig.ts -c seeder",
      "seeder:revert": "npm run typeorm -- migration:revert --config ormconfig.ts -c seeder"
   },
   "author": "",
   "license": "MIT",
   "devDependencies": {
      "@types/axios": "^0.14.0",
      "@types/cheerio": "^0.22.21",
      "@types/crypto-js": "^3.1.47",
      "@types/koa": "^2.11.3",
      "@types/koa-bodyparser": "^4.3.0",
      "@types/koa-logger": "^3.1.1",
      "@types/koa__router": "^8.0.2",
      "@types/mysql": "^2.15.15",
      "@types/node": "^14.0.27",
      "@typescript-eslint/eslint-plugin": "^3.8.0",
      "@typescript-eslint/parser": "^3.8.0",
      "eslint": "^7.6.0",
      "eslint-config-airbnb": "^18.2.0",
      "eslint-config-airbnb-base": "^14.2.0",
      "eslint-plugin-import": "^2.22.0",
      "ts-node": "^8.10.2",
      "ts-node-dev": "^1.0.0-pre.60",
      "typescript": "^3.9.7"
   },
   "dependencies": {
      "@koa/router": "^9.3.1",
      "axios": "^0.19.2",
      "cheerio": "^1.0.0-rc.3",
      "crypto-js": "^4.0.0",
      "dotenv": "^8.2.0",
      "koa": "^2.13.0",
      "koa-bodyparser": "^4.3.0",
      "koa-logger": "^3.2.1",
      "moment": "^2.27.0",
      "moment-timezone": "^0.5.31",
      "mysql": "^2.18.1",
      "reflect-metadata": "^0.1.10",
      "typeorm": "0.2.25"
   }
}